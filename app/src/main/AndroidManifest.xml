<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.samplerecyclerview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.samplerecyclerview.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.samplerecyclerview.simplerecycler.SimpleRecyclerActivity"
            android:label="Simple-type Example"
            android:parentActivityName="com.example.samplerecyclerview.HomeActivity" />
        <activity
            android:name="com.example.samplerecyclerview.simplerecyclerwithpagination.SimpleRecyclerWithPaginationActivity"
            android:label="Pagination Example"
            android:parentActivityName="com.example.samplerecyclerview.HomeActivity" />
        <activity
            android:name="com.example.samplerecyclerview.simplerecyclerwithmultitype.SimpleRecyclerWithMultiTypeActivity"
            android:label="Multi-type Example"
            android:parentActivityName="com.example.samplerecyclerview.HomeActivity" />
        <activity
            android:name="com.example.samplerecyclerview.staggeredrecycler.StaggeredRecyclerActivity"
            android:label="Staggered-type Example"
            android:parentActivityName="com.example.samplerecyclerview.HomeActivity" />
    </application>

</manifest>